{
  "name": "c2pa-generator-product",
  "version": "1.0.0",
  "description": "C2PA Generator Product Certification Assistant - An 8-bit Atari-styled AI assistant",
  "scripts": {
    "start": "./scripts/start.sh",
    "dev": "docker-compose up",
    "build": "docker-compose build",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f",
    "clean": "docker-compose down -v && rm -rf data/*.db logs/*.log",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "validate:data": "node scripts/validate-data-persistence.js",
    "validate:data:docker": "docker-compose exec backend node /app/scripts/validate-data-persistence.js",
    "lint": "cd backend && npm run lint && cd ../frontend && npm run lint",
    "find-port": "node scripts/find-port.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smsmatt/c2pa-generator-product.git"
  },
  "keywords": [
    "c2pa",
    "certification",
    "generator-product",
    "ai-assistant",
    "8-bit",
    "atari",
    "retro"
  ],
  "author": "Sanmarcsoft LLC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smsmatt/c2pa-generator-product/issues"
  },
  "homepage": "https://github.com/smsmatt/c2pa-generator-product#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
